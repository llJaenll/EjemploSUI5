<mvc:View
	controllerName="NombProy.Nomb_Proy.controller.Inicio"
	xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true"
	xmlns:l="sap.ui.layout"
	xmlns="sap.m">
	<App id="idAppControl">
		<pages>
			<Page title="{i18n>title}">
				<content>
						<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{i18n>TituloInicioGenericTileT}"
							subheader="" frameType="OneByOne" press="onTracking">
							<TileContent unit="" footer="">
								<ImageContent src="sap-icon://accounting-document-verification" />
							</TileContent>
						</GenericTile>
						
						<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{i18n>TituloInicioGenericTileC}"
							subheader="" frameType="OneByOne" press="onConsulta">
							<TileContent unit="" footer="">
								<ImageContent src="sap-icon://accounting-document-verification" />
							</TileContent>
						</GenericTile>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>